{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI Service Template",
    "description": "A template for creating FastAPI microservices",
    "version": "1.0.0"
  },
  "paths": {
    "/example/process": {
      "post": {
        "tags": [
          "example"
        ],
        "summary": "Process Example",
        "description": "Example endpoint that processes input data.\nReplace this with your actual business logic.",
        "operationId": "process_example_example_process_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ExampleBody"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExampleResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/example/status": {
      "get": {
        "tags": [
          "example"
        ],
        "summary": "Get Status",
        "description": "Example GET endpoint",
        "operationId": "get_status_example_status_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "summary": "Root",
        "description": "Health check endpoint",
        "operationId": "root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "summary": "Health",
        "description": "Health check endpoint",
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/api.json": {
      "get": {
        "summary": "Get Openapi Json",
        "description": "Endpoint to retrieve the OpenAPI specification in JSON format",
        "operationId": "get_openapi_json_api_json_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ExampleBody": {
        "properties": {
          "input": {
            "$ref": "#/components/schemas/ExampleInput"
          }
        },
        "type": "object",
        "required": [
          "input"
        ],
        "title": "ExampleBody"
      },
      "ExampleInput": {
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "value": {
            "type": "string",
            "title": "Value"
          }
        },
        "type": "object",
        "required": [
          "name",
          "value"
        ],
        "title": "ExampleInput"
      },
      "ExampleResponse": {
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "value": {
            "type": "string",
            "title": "Value"
          },
          "message": {
            "type": "string",
            "title": "Message",
            "default": "Success"
          }
        },
        "type": "object",
        "required": [
          "name",
          "value"
        ],
        "title": "ExampleResponse"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}